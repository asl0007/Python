while True:
    try:
        hello = int(input("What is your fav number?\n"))
        print(18/hello * hello / 18 + hello -1)
        break
    except ValueError:
        print("Make sure you enter a number")
    except ZeroDivisionError:
        print("enter a number other than zero")
    except:
        break
    finally:
        print("Do nothing")
